    "@iconify/react": "^6.0.2",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.17.24",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.5.9",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "postcss": "^8.5.3",
